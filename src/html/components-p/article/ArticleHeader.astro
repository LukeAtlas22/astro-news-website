---
import { getMetaArticleTemplate } from './typescript/utils/getMetaArticleTemplate';
import type { ArticleDataOBJ } from './typescript/types/ArticleDataOBJ';

interface Props {
  ArticleDataOBJ: ArticleDataOBJ,
  MetaLayoutType: string,
  MetaLayoutTemplate?: number,
}

const { ArticleDataOBJ, MetaLayoutType, MetaLayoutTemplate = 0 } = Astro.props;
const ArticleMeta = await getMetaArticleTemplate(MetaLayoutType, MetaLayoutTemplate);
---
<header class="article__header">
  <ArticleMeta article={ArticleDataOBJ}/>
  
</header>