---
import { getMetaArticleTemplate } from './typescript/utils/getArticleTemplate';
import type { ArticleDataOBJ } from './typescript/types/ArticleDataOBJ';

interface Props {
  ArticleDataOBJ: ArticleDataOBJ,
  MetaLayoutTemplate?: number,
}

const { ArticleDataOBJ, MetaLayoutTemplate = 0, } = Astro.props;
const ArticleMeta = await getMetaArticleTemplate(MetaLayoutTemplate);
---
<footer class="article__footer">
  <ArticleMeta article={ArticleDataOBJ}/>
</footer>